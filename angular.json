{
    "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
    "version": 1,
    "newProjectRoot": "projects",
    "projects": {
        "rare-lib-type-orm": {
            "projectType": "library",
            "root": "projects/rare-lib-type-orm",
            "sourceRoot": "projects/rare-lib-type-orm/src",
            "prefix": "rarelib",
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-angular:ng-packagr",
                    "options": {
                        "tsConfig": "projects/rare-lib-type-orm/tsconfig.lib.json",
                        "project": "projects/rare-lib-type-orm/ng-package.json"
                    },
                    "configurations": {
                        "production": {
                            "tsConfig": "projects/rare-lib-type-orm/tsconfig.lib.prod.json"
                        }
                    }
                },
                "test": {
                    "builder": "@angular-devkit/build-angular:karma",
                    "options": {
                        "polyfills": "projects/rare-lib-type-orm/src/polyfills.ts",
                        "main": "projects/rare-lib-type-orm/src/test.ts",
                        "tsConfig": "projects/rare-lib-type-orm/tsconfig.spec.json",
                        "karmaConfig": "projects/rare-lib-type-orm/karma.conf.js"
                    }
                },
                "lint": {
                    "builder": "@angular-eslint/builder:lint",
                    "options": {
                        "lintFilePatterns": [
                            "projects/rare-lib-type-orm/src/**/*.ts",
                            "projects/rare-lib-type-orm/src/**/*.html"
                        ]
                    }
                }
            }
        }
    },
    "cli": {
        "analytics": false
    },
    "defaultProject": "rare-lib-common"
}
